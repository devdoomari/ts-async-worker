{
  "name": "ts-async-worker",
  "version": "0.0.1",
  "description": "Typescript-focused async worker",
  "main": "lib/index.js",
  "module": "lib_es6/index.js",
  "license": "Mine",
  "scripts": {
    "clean": "rm -rf lib lib_es6 ts_processed",
    "test": "karma start",
    "compile_ts": "tsc --project tsconfig.lib.json",
    "compile_es6": "babel lib_es6 --out-dir lib",
    "copy_dts": "copyfiles -u 1 'lib_es6/**/*.d.ts' lib",
    "build": "npm run compile_ts && npm run compile_es6 && npm run copy_dts"
  },
  "devDependencies": {
    "@types/chai": "^4.0.1",
    "@types/karma": "^0.13.36",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-plugin-transform-regenerator": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.0",
    "jasmine-core": "^2.7.0",
    "karma": "^1.7.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript": "^3.0.4",
    "karma-webpack": "^2.0.4",
    "rollup": "^0.45.2",
    "ts-loader": "^2.3.2",
    "typescript": "^2.4.2",
    "webpack": "^3.4.1"
  },
  "dependencies": {
    "@types/jasmine": "^2.5.53",
    "@types/lodash": "^4.14.71",
    "@types/node": "^8.0.17",
    "@types/stacktrace-js": "^0.0.32",
    "@types/uuid": "^3.4.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "copyfiles": "^1.2.0",
    "fp-ts": "^0.4.3",
    "lodash": "^4.17.4",
    "stacktrace-js": "^2.0.0",
    "uuid": "^3.1.0"
  }
}
